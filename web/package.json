{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -H 127.0.0.1 -p 6161",
    "build": "next build",
    "build:gh-pages": "GITHUB_PAGES=true next build",
    "start": "NEXT_DISABLE_RESPONSE_CACHE=1 next start -H 0.0.0.0 -p $PORT",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:blog": "next lint --dir src/app/blog",
    "type-check": "tsc --noEmit",
    "pre-commit": "npm run lint:blog",
    "pre-push": "npm run lint && npm run build",
    "check-build": "npm run lint && npm run type-check && npm run build"
  },
  "engines": {
    "node": "^20 || ^22"
  },
  "dependencies": {
    "next": "15.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "resend": "^6.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "daisyui": "^5.0.50",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "typescript": "^5"
  }
}
